{
  "name": "excelmind-ai",
  "private": true,
  "version": "1.0.0",
  "description": "基于智谱AI的智能Excel处理工具",
  "main": "public/electron.cjs",
  "homepage": "./",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build && node scripts/analyze-bundle.js",
    "preview": "vite preview",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 http://localhost:3001 http://localhost:3002 http://localhost:3003 http://localhost:3004 http://localhost:3005 && set NODE_ENV=development&& electron .\"",
    "electron-pack": "npm run build && electron-builder",
    "dist": "npm run build && electron-builder --publish=never",
    "dist-portable": "npm run build && electron-builder --publish=never --win portable",
    "dist-zip": "npm run build && electron-builder --publish=never --win zip",
    "custom-build": "npm run build && npm run package-electron",
    "package-electron": "node scripts/package-electron.js",
    "test": "jest",
    "test:unit": "jest --selectProjects services",
    "test:component": "jest --selectProjects components",
    "test:integration": "jest --testPathPattern=integration",
    "test:regression": "jest --testPathPattern=regression",
    "test:performance": "jest --testPathPattern=performance",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:check-coverage": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":85,\"functions\":95,\"lines\":90,\"statements\":90}}'",
    "test:all": "jest",
    "test:verbose": "jest --verbose",
    "test:clear-cache": "jest --clearCache",
    "test:e2e": "node scripts/run-e2e-tests.js",
    "test:e2e:headless": "npx playwright test --headed=false",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:debug": "npx playwright test --debug",
    "test:generate-files": "node scripts/generate-test-files.js",
    "setup-resources": "node scripts/setup-resources.js",
    "test:agentic": "node scripts/run-agentic-tests.cjs all",
    "test:agentic:basic": "node scripts/run-agentic-tests.cjs basic",
    "test:agentic:otae": "node scripts/run-agentic-tests.cjs otae",
    "test:agentic:error-repair": "node scripts/run-agentic-tests.cjs error-repair",
    "test:agentic:mode-compare": "node scripts/run-agentic-tests.cjs mode-compare",
    "test:agentic:quality": "node scripts/run-agentic-tests.cjs quality",
    "test:agentic:multistep": "node scripts/run-agentic-tests.cjs multistep",
    "test:agentic:report": "node scripts/run-agentic-tests.cjs report",
    "test:agentic:benchmark": "npx playwright test tests/e2e/performance-benchmark.spec.ts",
    "test:backend": "node scripts/run-backend-tests.cjs",
    "build:backend": "node scripts/build-backend.mjs",
    "build:frontend": "npm run build && electron-builder --publish=never --dir",
    "build:all": "npm run build:backend && npm run build:frontend",
    "test:backend:storage": "node scripts/run-backend-tests.cjs storage",
    "test:backend:vfs": "node scripts/run-backend-tests.cjs vfs",
    "test:backend:coverage": "node scripts/run-backend-tests.cjs coverage",
    "perf:quick": "node scripts/run-performance-benchmark.cjs --quick",
    "perf:full": "node scripts/run-performance-benchmark.cjs --full",
    "perf:compare": "node scripts/run-performance-benchmark.cjs --compare --report",
    "perf:baseline": "node scripts/run-performance-benchmark.cjs --update-baseline",
    "test:pyodide-memory": "node scripts/run-pyodide-memory-test.cjs",
    "test:websocket": "npx tsx scripts/test-websocket.ts",
    "test:websocket:stability": "node tests/websocket/WebSocketStabilityTest.ts",
    "test:websocket:performance": "node tests/websocket/WebSocketPerformanceTest.ts",
    "test:websocket:server": "node tests/websocket/WebSocketTestServer.ts",
    "perf:test": "node scripts/run-performance-tests.cjs",
    "perf:test:quick": "node scripts/run-performance-tests.cjs --quick",
    "perf:test:baseline": "node scripts/run-performance-tests.cjs --update-baseline",
    "perf:test:compare": "node scripts/run-performance-tests.cjs --compare",
    "test:week1": "node scripts/run-week1-tests.cjs",
    "test:shared-types": "jest packages/shared-types/__tests__",
    "test:degradation": "jest services/infrastructure/degradation/__tests__",
    "test:phase2": "vitest run",
    "test:phase2:watch": "vitest --watch",
    "test:phase2:ui": "vitest --ui",
    "test:phase2:coverage": "vitest run --coverage",
    "test:unit:services": "vitest run tests/unit/services",
    "test:unit:api": "vitest run tests/unit/api",
    "test:integration:phase2": "vitest run tests/integration",
    "test:integration:api": "vitest run tests/integration/api.integration.test.ts",
    "test:e2e:phase2": "playwright test tests/e2e/dataQuality.spec.ts",
    "test:e2e:template": "playwright test tests/e2e/templateManagement.spec.ts",
    "test:e2e:batch": "playwright test tests/e2e/batchGeneration.spec.ts",
    "dev:api": "tsx watch server/dev-server.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:api\"",
    "dev:mock": "tsx scripts/mock-api-server.ts",
    "server:start": "tsx server/dev-server.ts",
    "server:websocket": "tsx scripts/start-websocket-server.ts",
    "server:websocket:test": "vitest run server/websocket/websocketServer.test.ts",
    "test:storage": "npx tsx scripts/test-storage.ts",
    "test:api:health": "npx tsx scripts/test-api-health.ts",
    "test:all:quick": "npm run test:storage && npm run test:api:health",
    "test:data:generate": "npx tsx scripts/generate-day2-test-data.ts",
    "test:audit": "node scripts/run-audit-assistant-test.cjs",
    "test:audit:data": "node scripts/generate-audit-test-data.cjs",
    "test:e2e:audit": "playwright test tests/e2e/audit-assistant-e2e.spec.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.0",
    "@monaco-editor/react": "^4.7.0",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-query-devtools": "^5.91.2",
    "alasql": "^4.16.0",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "docx-templates": "^4.15.0",
    "docxtemplater": "^3.67.6",
    "docxtemplater-image-module-free": "^1.1.1",
    "exceljs": "^4.4.0",
    "helmet": "^7.1.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.561.0",
    "mammoth": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "node-pty": "^1.1.0",
    "pdfjs-dist": "^5.4.530",
    "pizzip": "^3.2.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-dropzone": "^14.3.8",
    "react-markdown": "^10.1.0",
    "recharts": "^3.6.0",
    "sql-formatter": "^15.6.12",
    "tailwind-merge": "^3.4.0",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "^2.1.12",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.19.7",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.18.1",
    "@vercel/ncc": "^0.38.4",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.22",
    "concurrently": "^8.2.2",
    "electron": "^40.0.0",
    "electron-builder": "^26.5.0",
    "esbuild": "^0.27.3",
    "express": "^4.18.2",
    "fake-indexeddb": "^6.2.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.5",
    "supertest": "^7.2.2",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0",
    "typescript": "~5.8.2",
    "vite": "6.2.0",
    "vitest": "^2.0.0",
    "wait-on": "^7.2.0",
    "ws": "^8.19.0"
  },
  "build": {
    "appId": "com.excelmind.ai",
    "productName": "ExcelMind AI",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "public/electron.cjs",
      "public/preload.js",
      "public/electron/**/*",
      "node_modules/**/*"
    ],
    "extraResources": [
      {
        "from": "node_modules/@anthropic-ai/claude-code",
        "to": "bin/claude-code",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "resources/bin/node.exe",
        "to": "bin/node.exe"
      },
      {
        "from": "resources/bin/node",
        "to": "bin/node"
      }
    ],
    "asarUnpack": [
      "**/node-pty/**",
      "public/electron/**/*"
    ],
    "npmRebuild": false,
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  },
  "overrides": {
    "xmldom": "npm:@xmldom/xmldom@^0.8.11",
    "tar": "^6.2.1",
    "@electron/rebuild": {
      "tar": "^6.2.1"
    },
    "@mapbox/node-pre-gyp": {
      "tar": "^6.2.1"
    },
    "app-builder-lib": {
      "tar": "^6.2.1"
    }
  }
}